generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model product {
  id        Int       @id
  name      String?
  price     Decimal?  @db.Money
  category  String?
  image     String?
  available Boolean?
  quantity  Int?
  createdat DateTime?
}
